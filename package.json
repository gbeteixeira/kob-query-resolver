{
  "name": "@kobi-tech/kobi-query-resolver",
  "version": "1.0.4",
  "description": "A flexible query resolver for dynamic validations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "biome check --write src/ && biome check --write test/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:ui": "vitest --ui",
    "prepare": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gbeteixeira/kobi-query-resolver.git"
  },
  "keywords": [
    "equation",
    "resolver",
    "validation",
    "query"
  ],
  "author": "Gabriel Teixeira",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "vitest": "^2.1.8",
    "zod": "^3.23.8"
  }
}
